{
  "name": "ritsu",
  "version": "0.0.1",
  "description": "An api to collect bearer token to be used via Discord Oatuh2",
  "main": "build",
  "typings": "types",
  "scripts": {
    "build": "tsc",
    "dev:build": "tsc -w",
    "prettier": "prettier **.json src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "generate:changelog": "auto-changelog",
    "generate:doc": "typedoc --options typedoc.json"
  },
  "keywords": [
    "nodejs",
    "discord",
    "discord-bot",
    "discord-api"
  ],
  "dependencies": {
    "axios": "^0.26.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "4.5.5",
    "auto-changelog": "^2.4.0",
    "typedoc": "^0.22.12"
  },
  "auto-changelog": {
    "output": "HISTORY.md",
    "template": "changelog-template.hbs",
    "package": true,
    "commitLimit": false
  },
  "contributors": [
    "fantomitechno <fantomitechno@gmail.com>"
  ],
  "author": "fantomitechno <fantomitechno@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitsune-js/Ritsu.git"
  },
  "homepage": "https://kitsune-js.github.io/Ritsu",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.0.0"
}
